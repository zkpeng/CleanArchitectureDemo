<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="demo.zkp.com.weixiaoclean">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.UserListActivity"
            android:theme="@style/Base.Theme.AppCompat"></activity>
        <activity
            android:name=".activity.MainActivity"
            android:theme="@style/Base.Theme.AppCompat">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.BaseActivity"
            android:theme="@style/Base.Theme.AppCompat" />
        <activity android:name=".activity.UserDetailActivity" />
        <activity android:name=".activity.UserDetailsActivity" />
    </application>

</manifest>